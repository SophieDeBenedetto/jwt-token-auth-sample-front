<div class="container">

  <h1>Ember JWT Token Auth Example App</h1>

  {{#if session.isAuthenticated}}
    <li><a href="#">Hi, {{sessionAccount.account.email}}</a></li>
    <li><a href="#" {{action 'invalidateSession'}}>Logout</a></li>
  {{else}}
    <li>{{#link-to 'login'}}Log In{{/link-to}}</li>
    <li>{{#link-to 'signup'}}Sign Up{{/link-to}}</li>
  {{/if}}
  {{outlet}}
</div>


